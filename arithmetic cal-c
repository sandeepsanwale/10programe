#include<stdio.h>
#include<conio.h>
int main()
{
int a,b;
int op;
printf("1.addition \n 2. substraction \n 3.multiplication \n 4. division \n");
printf("enter the value of a&b:");
scanf("%d%d",&a,&b);
printf("enter your choice");
scanf("%d",&op);
switch(op)
{
	1; printf("addition of %d and %d is:%d",a+b);
	break;
	2; printf("substraction of %d and %d is: %d",a-b);
	break;
	3; printf("multiplication of %d and %d is: %d",a*b);
	break;
	4; printf("division of %da and %d is:%d",a/b);
	break;
    //default:
		}
		return 0;
		getch();
     } 
